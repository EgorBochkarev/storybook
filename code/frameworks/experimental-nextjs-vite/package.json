{
  "name": "@storybook/experimental-nextjs-vite",
  "version": "8.5.0-alpha.4",
  "description": "Storybook for Next.js and Vite",
  "keywords": [
    "storybook",
    "nextjs",
    "vite"
  ],
  "homepage": "https://github.com/storybookjs/storybook/tree/next/code/frameworks/experimental-nextjs-vite",
  "bugs": {
    "url": "https://github.com/storybookjs/storybook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/storybook.git",
    "directory": "code/frameworks/nextjs"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  },
  "license": "MIT",
  "sideEffects": [
    "./src/vite-plugin/polyfills/promise-with-resolvers.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.js"
    },
    "./dist/preview.mjs": "./dist/preview.mjs",
    "./dist/compatibility/draft-mode.compat": {
      "types": "./dist/compatibility/draft-mode.compat.d.ts",
      "import": "./dist/compatibility/draft-mode.compat.mjs",
      "require": "./dist/compatibility/draft-mode.compat.js"
    },
    "./cache.mock": {
      "types": "./dist/export-mocks/cache/index.d.ts",
      "import": "./dist/export-mocks/cache/index.mjs",
      "require": "./dist/export-mocks/cache/index.js"
    },
    "./headers.mock": {
      "types": "./dist/export-mocks/headers/index.d.ts",
      "import": "./dist/export-mocks/headers/index.mjs",
      "require": "./dist/export-mocks/headers/index.js"
    },
    "./navigation.mock": {
      "types": "./dist/export-mocks/navigation/index.d.ts",
      "import": "./dist/export-mocks/navigation/index.mjs",
      "require": "./dist/export-mocks/navigation/index.js"
    },
    "./router.mock": {
      "types": "./dist/export-mocks/router/index.d.ts",
      "import": "./dist/export-mocks/router/index.mjs",
      "require": "./dist/export-mocks/router/index.js"
    },
    "./vite-plugin-mocks/browser/mocks/cache": "./dist/vite-plugin/plugins/next-mocks/alias/cache/index.mjs",
    "./vite-plugin-mocks/browser/mocks/navigation": "./dist/vite-plugin/plugins/next-mocks/alias/navigation/index.mjs",
    "./vite-plugin-mocks/browser/mocks/headers": "./dist/vite-plugin/plugins/next-mocks/alias/headers/index.mjs",
    "./vite-plugin-mocks/browser/mocks/router": "./dist/vite-plugin/plugins/next-mocks/alias/router/index.mjs",
    "./vite-plugin-mocks/browser/mocks/server-only": "./dist/vite-plugin/plugins/next-mocks/alias/rsc/server-only.mjs",
    "./vite-plugin-mocks/browser/mocks/dynamic": "./dist/vite-plugin/plugins/next-mocks/alias/dynamic/index.mjs",
    "./vite-plugin-mocks/browser/mocks/image": "./dist/vite-plugin/plugins/next-image/alias/next-image.mjs",
    "./vite-plugin-mocks/browser/mocks/legacy-image": "./dist/vite-plugin/plugins/next-image/alias/next-legacy-image.mjs",
    "./vite-plugin-mocks/browser/mocks/image-default-loader": "./dist/vite-plugin/plugins/next-image/alias/image-default-loader.js",
    "./vite-plugin-mocks/browser/mocks/image-context": "./dist/vite-plugin/plugins/next-image/alias/image-context.mjs",
    "./vite-plugin-mocks/node/mocks/cache": "./dist/vite-plugin/plugins/next-mocks/alias/cache/index.js",
    "./vite-plugin-mocks/node/mocks/navigation": "./dist/vite-plugin/plugins/next-mocks/alias/navigation/index.js",
    "./vite-plugin-mocks/node/mocks/headers": "./dist/vite-plugin/plugins/next-mocks/alias/headers/index.js",
    "./vite-plugin-mocks/node/mocks/router": "./dist/vite-plugin/plugins/next-mocks/alias/router/index.js",
    "./vite-plugin-mocks/node/mocks/server-only": "./dist/vite-plugin/plugins/next-mocks/alias/rsc/server-only.js",
    "./vite-plugin-mocks/node/mocks/dynamic": "./dist/vite-plugin/plugins/next-mocks/alias/dynamic/index.js",
    "./vite-plugin-mocks/node/mocks/image": "./dist/vite-plugin/plugins/next-image/alias/next-image.js",
    "./vite-plugin-mocks/node/mocks/legacy-image": "./dist/vite-plugin/plugins/next-image/alias/next-legacy-image.js",
    "./vite-plugin-mocks/node/mocks/image-default-loader": "./dist/vite-plugin/plugins/next-image/alias/image-default-loader.js",
    "./vite-plugin-mocks/node/mocks/image-context": "./dist/vite-plugin/plugins/next-image/alias/image-context.js",
    "./vite-plugin-mocks": {
      "types": "./dist/vite-plugin/index.d.ts",
      "import": "./dist/vite-plugin/index.mjs",
      "require": "./dist/vite-plugin/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "cache.mock": [
        "dist/export-mocks/cache/index.d.ts"
      ],
      "headers.mock": [
        "dist/export-mocks/headers/index.d.ts"
      ],
      "router.mock": [
        "dist/export-mocks/router/index.d.ts"
      ],
      "navigation.mock": [
        "dist/export-mocks/navigation/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "template/cli/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "check": "jiti ../../../scripts/prepare/check.ts",
    "prep": "jiti ../../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@next/env": "^15.0.3",
    "@storybook/builder-vite": "workspace:*",
    "@storybook/react": "workspace:*",
    "@storybook/test": "workspace:*",
    "@types/semver": "^7.3.4",
    "image-size": "^1.1.1",
    "magic-string": "^0.30.11",
    "module-alias": "^2.2.3",
    "semver": "^7.3.5",
    "styled-jsx": "5.1.6",
    "ts-dedent": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "next": "^15.0.3",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "@storybook/test": "workspace:*",
    "next": "^14.1.0 || ^15.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0-beta",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0-beta",
    "storybook": "workspace:^",
    "vite": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "sharp": "^0.33.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/vite-plugin/index.ts",
      "./src/preset.ts",
      "./src/preview.tsx",
      "./src/export-mocks/cache/index.ts",
      "./src/export-mocks/headers/index.ts",
      "./src/export-mocks/router/index.ts",
      "./src/export-mocks/navigation/index.ts",
      "./src/images/decorator.tsx",
      "./src/vite-plugin/plugins/next-image/alias/next-image.tsx",
      "./src/vite-plugin/plugins/next-image/alias/next-legacy-image.tsx",
      "./src/vite-plugin/plugins/next-image/alias/image-default-loader.tsx",
      "./src/vite-plugin/plugins/next-image/alias/image-context.tsx",
      "./src/vite-plugin/plugins/next-mocks/alias/headers/cookies.ts",
      "./src/vite-plugin/plugins/next-mocks/alias/headers/headers.ts",
      "./src/vite-plugin/plugins/next-mocks/alias/headers/index.ts",
      "./src/vite-plugin/plugins/next-mocks/alias/cache/index.ts",
      "./src/vite-plugin/plugins/next-mocks/alias/navigation/index.ts",
      "./src/vite-plugin/plugins/next-mocks/alias/router/index.ts",
      "./src/vite-plugin/plugins/next-mocks/alias/rsc/server-only.ts",
      "./src/vite-plugin/plugins/next-mocks/alias/dynamic/index.tsx",
      "./src/vite-plugin/mocks/storybook.global.ts"
    ],
    "externals": [
      "sb-original/image-context",
      "sb-original/default-loader",
      "storybook",
      "@storybook/test-runner",
      "@storybook/nextjs",
      "@storybook/test",
      "react",
      "sharp",
      "next"
    ],
    "platform": "node"
  },
  "gitHead": "e6a7fd8a655c69780bc20b9749c2699e44beae16"
}
